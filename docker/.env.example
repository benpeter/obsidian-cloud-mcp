# Obsidian REST API Key
# Get this from Obsidian -> Settings -> Local REST API -> API Key
OBSIDIAN_API_KEY=your_api_key_here

# JWT Secret for MCP Authentication (min 32 characters)
# Generate with: openssl rand -base64 48
MCP_JWT_SECRET=your_jwt_secret_min_32_chars_here

# Password for Web GUI (VNC) access
# Generate with: openssl rand -base64 16
VNC_PASSWORD=your_vnc_password_here
