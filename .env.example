# Hetzner Cloud API Token
# Get it from: https://console.hetzner.cloud/projects/YOUR_PROJECT/security/tokens
HCLOUD_TOKEN=your_hetzner_api_token_here

# VNC Password for GUI access (min 6 characters)
VNC_PASSWORD=your_vnc_password_here

# Obsidian REST API Key (set after manual Obsidian setup)
# This is configured in Obsidian's Local REST API plugin
OBSIDIAN_API_KEY=your_api_key_here
